generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                   String @id
  email                String @unique
  name                 String
  lastTokenHash        String @unique
  findWinnerCount      Int    @default(0)
  suggestNextMoveCount Int    @default(0)
}
